
  <%# <%= "Please input correct fields" %>
  <div class="card">
    <ul class="list-group-item list-group-flush">
    <%= form_tag url_for(action: 'create'), method: "post" do %>

  <li class="list-group-item">
<%= label_tag 'Email' %>
<%= text_field_tag 'email', @email, class: 'form-control' %>
</li>
  <li class="list-group-item">
    <%= label_tag 'Choose your password' %>
      <%= text_field_tag 'password', @password, class: 'form-control', type: "password" %>
        </li>
    <li class="list-group-item">
      <%= submit_tag "Log in", class: 'btn' %>
      </li>
      <% end %>
    </ul>
  </div>
